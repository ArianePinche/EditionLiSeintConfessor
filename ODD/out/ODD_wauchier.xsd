      <xs:attributeGroup ref="ns1:att.global.responsibility.attribute.resp"/>
      <xs:attributeGroup ref="ns1:att.typed.attribute.subtype"/>
      <xs:attributeGroup ref="ns1:att.sortable.attributes"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="location">
    <xs:annotation>
      <xs:documentation>defines the location of a place as a set of geographical coordinates, in terms of other named geo-political entities, or as an address. [13.3.4. Places]</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:group ref="ns1:model.placeNamePart"/>
        <xs:group ref="ns1:model.measureLike"/>
        <xs:element ref="ns1:model.noteLike"/>
