      <xs:attributeGroup ref="ns1:att.global.change.attribute.change"/>
      <xs:attributeGroup ref="ns1:att.global.responsibility.attribute.cert"/>
      <xs:attributeGroup ref="ns1:att.global.responsibility.attribute.resp"/>
      <xs:attributeGroup ref="ns1:att.typed.attribute.subtype"/>
      <xs:attributeGroup ref="ns1:att.sortable.attributes"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="listPlace">
    <xs:annotation>
      <xs:documentation>(list of places) contains a list of places, optionally followed by a list of relationships (other than containment) defined amongst them. [2.2.7. The Source Description 13.3.4. Places]</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:group minOccurs="0" maxOccurs="unbounded" ref="ns1:model.headLike"/>
        <xs:choice maxOccurs="unbounded">
          <xs:element ref="ns1:model.placeLike"/>
          <xs:element ref="ns1:listPlace"/>
