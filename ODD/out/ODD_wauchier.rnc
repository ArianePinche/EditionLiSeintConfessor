    att.declaring.attributes,
    att.typed.attribute.subtype,
    empty
  }
body =
  
  ## (text body) contains the whole body of a single unitary text, excluding any front or back matter. [4. Default Text Structure]
  element body {
    (model.global*,
     (model.divTop, (model.global | model.divTop)*)?,
     (model.divGenLike, (model.global | model.divGenLike)*)?,
     ((model.divLike, (model.global | model.divGenLike)*)+
      | (model.div1Like, (model.global | model.divGenLike)*)+
      | ((model.common, model.global*)+,
         ((model.divLike, (model.global | model.divGenLike)*)+
          | (model.div1Like, (model.global | model.divGenLike)*)+)?)),
     (model.divBottom, model.global*)*),
    att.global.attribute.xmlid,
    att.global.attribute.n,
    att.global.attribute.xmllang,
    att.global.attribute.xmlbase,
    att.global.attribute.xmlspace,
    att.global.rendition.attribute.rend,
    att.global.rendition.attribute.style,
    att.global.rendition.attribute.rendition,
    att.global.change.attribute.change,
    att.global.responsibility.attribute.cert,
    att.global.responsibility.attribute.resp,
    att.declaring.attributes,
    empty
  }
\div =
  
  ## (Divisions principales) permet de structurer les différents éléments du texte. [4.1. Divisions of the Body]
  element div {
    ((model.divTop | model.global)*,
     ((((model.divLike | model.divGenLike), model.global*)+
       | ((model.common, model.global*)+,
          ((model.divLike | model.divGenLike), model.global*)*)),
      (model.divBottom, model.global*)*)?)
    >> sch:pattern [
         id = "oddbyexample-div-divNumber-constraint-11"
         "\x{a}" ~
         "            "
         s:rule [
           context = "tei:div[@n]"
           "\x{a}" ~
           "                                    "
           s:assert [
             test =
               "number(@n) = number(preceding-sibling::tei:div[1]/@n) + 1 or count(preceding-sibling::tei:div[1]/@n) = 0"
             " value of @n is wrong "
           ]
           "\x{a}" ~
           "                                "
         ]
         "\x{a}" ~
         "         "
       ]
    >> sch:pattern [
         id = "oddbyexample-div-divChapter-constraint-12"
         "\x{a}" ~
         "            "
         s:rule [
           context = "tei:div[@type='chapter']"
           "\x{a}" ~
           "                                    "
           s:assert [
             test = "tei:div[@type='section']"
             " div[@type='chapter']\x{a}" ~
             "                                        includes only div[@type='section'] "
           ]
           "\x{a}" ~
           "                                "
         ]
         "\x{a}" ~
         "         "
       ]
    >> sch:pattern [
         id = "oddbyexample-div-divSection-constraint-13"
         "\x{a}" ~
         "            "
         s:rule [
           context = "tei:div[@type='section']"
           "\x{a}" ~
           "                                    "
           s:assert [
             test = "tei:p and not(tei:div)"
             " div[@type='section']\x{a}" ~
             "                                        only includes p "
           ]
           "\x{a}" ~
           "                                "
         ]
         "\x{a}" ~
         "         "
       ],
    att.global.attribute.xmlid,
    att.global.attribute.xmllang,
    att.global.attribute.xmlbase,
    att.global.attribute.xmlspace,
    att.global.rendition.attribute.rend,
    att.global.rendition.attribute.style,
    att.global.rendition.attribute.rendition,
    att.global.linking.attribute.corresp,
    att.global.facs.attribute.facs,
    att.global.change.attribute.change,
    att.global.responsibility.attribute.cert,
    att.global.responsibility.attribute.resp,
    att.typed.attribute.subtype,
    att.declaring.attributes,
    
    ## (number) gives a number (or other label) for an element, which is not necessarily unique within the document.
    attribute n { data.text },
    
    ## characterizes the element in some sense, using any convenient classification scheme or typology.
    attribute type {
      
      ## (Chapitre) signale les divisions de premier niveau.
      "chapter"
      | 
        ## (Section) signale les divisions de deuxième niveau.
        "section"
    },
    empty
  }
